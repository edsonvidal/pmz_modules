<?xml version="1.0"?>
<root>
  <OBJECT NAME="TABELAS_PRECO">
    <METHOD NAME="Listar" DESCRIPTION="Lista Tabela de Pre&#231;os" VERSION="85" INTFTYPE="5" THREADSAFE="1">
      <PARAMS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <PARAM NAME="COD_TPRECO" SIZE="5" PROJECTION="0" FIELDLABEL="Cod Tpreco" CTAB="0"/>
      </PARAMS>
