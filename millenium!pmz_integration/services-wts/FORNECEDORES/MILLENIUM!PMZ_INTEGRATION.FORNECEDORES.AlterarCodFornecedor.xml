<?xml version="1.0"?>
<root>
  <OBJECT NAME="FORNECEDORES">
    <METHOD NAME="AlterarCodFornecedor" DESCRIPTION="Alterar o codigo do fornecedor de acordo com o da PMZ" VERSION="6" INTFTYPE="2" THREADSAFE="1">
      <PARAMS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <PARAM NAME="FORNECEDOR" FORMAT="N" SIZE="8" FLAGS="1" PROJECTION="0" FIELDLABEL="Fornecedor" CTAB="0"/>
        <PARAM NAME="COD_FORNECEDOR" SIZE="12" FLAGS="1" PROJECTION="0" FIELDLABEL="Cod Fornecedor" CTAB="0"/>
      </PARAMS>
      <FIELDS/>
      <ACTIONSCRIPT>#PRIVATE()
UPDATE IFORNECEDORES
SET    COD_FORNECEDOR = :COD_FORNECEDOR
WHERE (FORNECEDOR= :FORNECEDOR);

</ACTIONSCRIPT>
    </METHOD>
  </OBJECT>
</root>