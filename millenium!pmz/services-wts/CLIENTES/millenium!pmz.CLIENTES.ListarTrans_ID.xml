<?xml version="1.0"?>
<root>
  <OBJECT NAME="CLIENTES">
    <METHOD NAME="ListarTrans_ID" DESCRIPTION="Retornar Lista de Clientes pelo Trans ID" VERSION="20" INTFTYPE="5" THREADSAFE="1">
      <PARAMS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <PARAM NAME="TRANS_ID" FORMAT="N" SIZE="8" FLAGS="1" PROJECTION="0" ORDER="1" FIELDLABEL="Trans Id" CTAB="0"/>
      </PARAMS>
      <FIELDS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <FIELD NAME="CLIENTE" FORMAT="N" SIZE="8" FLAGS="1" PROJECTION="0" FIELDLABEL="Cliente" CTAB="0"/>
        <FIELD NAME="GERADOR" FORMAT="N" SIZE="8" FLAGS="1" PROJECTION="0" FIELDLABEL="Gerador" CTAB="0"/>
        <FIELD NAME="TRANS_ID" FORMAT="N" SIZE="8" FLAGS="1" PROJECTION="0" FIELDLABEL="Trans Id" CTAB="0"/>
        <FIELD NAME="CODIGOPMZ" SIZE="12" FLAGS="1" PROJECTION="0" FIELDLABEL="Codigopmz" CTAB="0"/>
      </FIELDS>
      <ACTIONSCRIPT>SELECT C.CLIENTE AS CLIENTE,
       C.GERADOR AS GERADOR,
       C.TRANS_ID AS TRANS_ID,
       C.COD_CLIENTE AS CODIGOPMZ,
       C.NOME AS NOME
FROM CLIENTES C
WHERE C.TRANS_ID > :TRANS_ID
ORDER BY TRANS_ID
</ACTIONSCRIPT>
    </METHOD>
  </OBJECT>
</root>